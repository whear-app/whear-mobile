{
  "name": "whear-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "node scripts/start-expo.js",
    "start:original": "expo start",
    "android": "node scripts/start-expo.js --android",
    "android:original": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,json}\"",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "prebuild:android": "expo prebuild --platform android",
    "prebuild:ios": "expo prebuild --platform ios",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android:debug": "cd android && ./gradlew assembleDebug",
    "build:ios": "cd ios && xcodebuild -workspace Whear.xcworkspace -scheme Whear -configuration Release",
    "run:android": "npx react-native run-android",
    "run:ios": "npx react-native run-ios",
    "run:ios:simulator": "npx react-native run-ios --simulator=\"iPhone 15 Pro\""
  },
  "dependencies": {
    "@expo-google-fonts/cormorant": "^0.4.2",
    "@expo/vector-icons": "^15.0.3",
    "@hookform/resolvers": "^3.3.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "expo": "~54.0.31",
    "expo-auth-session": "~7.0.10",
    "expo-blur": "^15.0.8",
    "expo-constants": "~18.0.13",
    "expo-crypto": "~15.0.8",
    "expo-font": "^14.0.10",
    "expo-image": "~3.0.11",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "^15.0.8",
    "expo-linking": "~8.0.11",
    "expo-location": "~19.0.8",
    "expo-status-bar": "~3.0.9",
    "i18next": "^25.7.4",
    "react": "^19.1.0",
    "react-hook-form": "^7.49.2",
    "react-i18next": "^16.5.3",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.11.3",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-vector-icons": "^10.0.3",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "babel-preset-expo": "~54.0.9",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "typescript": "~5.9.2"
  },
  "private": true
}
